    <!-- Trending container  -->
    <!-- Optional: Change to modal system to allow for review stars-->
    <!-- Known issue: Scaling on caption-text breaks on small browser sizes-->
    <div class="container">
        <div class="trending-grid">
            <div class="col-sm-12">
                <article class="trending">
                    <h4>Trending Notes:</h4>
                </article>  
                <div class="pixgrid clearfix">
                    <div class="row pb-2">
                        <% notesList.forEach(function(item){ %>
                        <figure class="figure col-3">
                            <img src="/images/courses/<%= item.image %>" class="figure-img rounded" alt="Notes 1">
                            <figcaption class="figure-caption d-inline-block text-truncate" style="max-width: 120px;"><%= item.name %></figcaption>
                        </figure>
                        <% }) %>
                    </div>
                </div>  
            </div>
        </div>
    </div>